<li><%= link_to 'Classrooms', classrooms_path %></li>

<% if @classroom.try(:id) %>
  <li id="track_link" data-classroomid="<%= @classroom.id %>" ><%= link_to 'Tracks', classroom_tracks_path(@classroom) %></li>
  <%= subscribe_to "/classrooms/#{@classroom.id}/requesters" %>
  <% if current_user.teacher? %>
    <%= render 'shared/teacher_nav' %>
  <% else %>
    <%= render 'shared/student_nav' %>
  <% end %>
<% end %>

<li><%= link_to 'Profile', edit_profile_path %></li>
<li class="hide-role"><span> <%= "Role: #{current_user.classrole_type}; #{current_user.email}" %> </span></li>
<li><%= link_to 'Logout', logout_path, method: :delete %></li>