<h6 class="subtitle classroom-option">Join Existing Classroom</h6>
<%= form_tag join_classrooms_path, method: :post do %>
  <div class="form-group">
    <%= label_tag :join_existing_classroom %>
    <%= text_field_tag :teacher_token, nil,placeholder: "Enter classroom token", class: "form-input" %>
  </div>
  <div>
    <%= submit_tag 'Join Classroom', class: "btn btn-primary" %>
  </div>
<% end %>